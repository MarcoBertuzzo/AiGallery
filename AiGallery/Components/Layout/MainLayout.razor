@inherits LayoutComponentBase
@inject TitleService TitleService

<PageTitle>@TitleService.GetTitle()</PageTitle>

    <HeadContent>
        <meta property="og:title" content="@TitleService.GetTitle()" id="ogTitle" />
    </HeadContent>


<TopMenu></TopMenu>

<div class="page">
        
    <main>
        <article class="content px-4">
            @Body
        </article>
    </main>
</div>

<div id="blazor-error-ui">
    An unhandled error has occurred.
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>
