{
  "AiGallery": {
    "SmtpClient": {
      "Host": "smtp server address",
      "Port": "587",
      "Username": "username",
      "Password": "password",
      "MailAddress": "your email address",
      "EnableSsl": true
    }
  },
  "Serilog": {
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "AiGallery.log"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=AiGallery.db"
  },

  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
